#Auto-generated config file for voice aholab-iparrahots

name = aholab-iparrahots
locale = en_US


####################################################################
####################### Module settings  ###########################
####################################################################
# For keys ending in ".list", values will be appended across config files,
# so that .list keys can occur in several config files.
# For all other keys, values will be copied to the global config, so
# keys should be unique across config files.

hmm.voices.list = \
   aholab-iparrahots

# If this setting is not present, a default value of 0 is assumed.
voice.aholab-iparrahots.wants.to.be.default = 0

# Set your voice specifications
voice.aholab-iparrahots.gender = female
voice.aholab-iparrahots.locale = en_US
voice.aholab-iparrahots.domain = general
voice.aholab-iparrahots.samplingRate = 48000
voice.aholab-iparrahots.framePeriod = 240

# HMM Voice-specific parameters 
# parameters used during models training 
# MGC: stage=gamma=0 alpha=0.42 16KHz linear gain; alpha=0.55 48Khz log gain (default) 
# LSP: gamma>0  
#          LSP: gamma=1 alpha=0.0  linear gain/log gain 
#      Mel-LSP: gamma=1 alpha=0.42 log gain 
#      MGC-LSP: gamma=3 alpha=0.42 log gain 
voice.aholab-iparrahots.alpha = 0.55
voice.aholab-iparrahots.gamma = 0
voice.aholab-iparrahots.logGain = true

# Parameter beta for postfiltering 
voice.aholab-iparrahots.beta = 0.1

# HMM Voice-specific files
# Information about trees
voice.aholab-iparrahots.Ftd = jar:/marytts/voice/AholabIparrahots/tree-dur.inf
voice.aholab-iparrahots.Ftf = jar:/marytts/voice/AholabIparrahots/tree-lf0.inf
voice.aholab-iparrahots.Ftm = jar:/marytts/voice/AholabIparrahots/tree-mgc.inf
voice.aholab-iparrahots.Fts = jar:/marytts/voice/AholabIparrahots/tree-str.inf

# Information about means and variances PDFs 
voice.aholab-iparrahots.Fmd = jar:/marytts/voice/AholabIparrahots/dur.pdf
voice.aholab-iparrahots.Fmf = jar:/marytts/voice/AholabIparrahots/lf0.pdf
voice.aholab-iparrahots.Fmm = jar:/marytts/voice/AholabIparrahots/mgc.pdf
voice.aholab-iparrahots.Fms = jar:/marytts/voice/AholabIparrahots/str.pdf

# Information about Global Mean and Variance PDFs
voice.aholab-iparrahots.useGV = true

# Variable for allowing context-dependent GV (without sil)
voice.aholab-iparrahots.useContextDependentGV = true

# GV method: gradient or derivative (default gradient)
voice.aholab-iparrahots.gvMethod = gradient

# Max number of GV iterations
voice.aholab-iparrahots.maxMgcGvIter = 100
voice.aholab-iparrahots.maxLf0GvIter = 100
voice.aholab-iparrahots.maxStrGvIter = 100

# GV weights for each parameter: between 0.0-2.0
voice.aholab-iparrahots.gvweightMgc = 1.0
voice.aholab-iparrahots.gvweightLf0 = 1.0
voice.aholab-iparrahots.gvweightStr = 1.0

# Mean and variance files for GV
voice.aholab-iparrahots.Fgvf = jar:/marytts/voice/AholabIparrahots/gv-lf0.pdf
voice.aholab-iparrahots.Fgvm = jar:/marytts/voice/AholabIparrahots/gv-mgc.pdf
voice.aholab-iparrahots.Fgvs = jar:/marytts/voice/AholabIparrahots/gv-str.pdf

# A context features file example for start-up testing.
voice.aholab-iparrahots.FeaFile = jar:/marytts/voice/AholabIparrahots/cmu_us_arctic_slt_b0487.pfeats

# Tricky phones file in case there were problematic phones during training, empty otherwise.
voice.aholab-iparrahots.trickyPhonesFile = jar:/marytts/voice/AholabIparrahots/trickyPhones.txt

# Information about Mixed Excitation
voice.aholab-iparrahots.useMixExc = true

# Filters for mixed excitation 
# File format: one column with all the taps, where the number of taps per filter = numTaps/numFilters 
voice.aholab-iparrahots.Fif = jar:/marytts/voice/AholabIparrahots/mix_excitation_filters.txt
# Number of filters in bandpass bank
voice.aholab-iparrahots.in = 5

# Information about acousticModels (if true allows prosody modification specified in MARYXML input)
voice.aholab-iparrahots.useAcousticModels = true

# acoustic models to use (HMM models or carts from other voices can be specified)
#(uncoment to allow prosody modification specified in MARYXML input)
voice.aholab-iparrahots.acousticModels = duration F0

voice.aholab-iparrahots.duration.model = hmm
# voice.aholab-iparrahots.duration.data not used for hmm models
voice.aholab-iparrahots.duration.attribute = d

voice.aholab-iparrahots.F0.model = hmm
# voice.aholab-iparrahots.F0.data not used for hmm models
voice.aholab-iparrahots.F0.attribute = f0

